{
  "name": "spamjs",
  "version": "1.1.0",
  "description": "A D3 library to create maps with Canvas",
  "keywords": [
    "canvas",
    "d3",
    "maps"
  ],
  "homepage": "https://github.com/newsappsio/spam",
  "license": "MIT",
  "author": {
    "name": "Lukas Appelhans & Martín González"
  },
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c"
  },
  "main": "dist/spam.js",
  "unpkg": "dist/spam.min.js",
  "jsdelivr": "dist/spam.min.js",
  "module": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/newsappsio/spam.git"
  },
  "dependencies": {
    "d3-ease": "1",
    "d3-geo": "1",
    "d3-interpolate": "3",
    "d3-selection": "1",
    "d3-transition": "3",
    "rbush": "3"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "0",
    "@rollup/plugin-node-resolve": "6",
    "rollup": "1",
    "rollup-plugin-filesize": "6",
    "rollup-plugin-terser": "5"
  }
}
